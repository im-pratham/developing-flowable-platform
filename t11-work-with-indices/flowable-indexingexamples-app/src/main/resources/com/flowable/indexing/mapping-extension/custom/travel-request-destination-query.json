{
  "key": "travel-request-destination-query",
  "name": "travel-request-destination-query",
  "type": "query",
  "sourceIndex": "tasks",
  "version": 1,
  "parameters": {
    "destination": "string"
  },
  "customFilter": {
    "bool": {
      "must": [
        {
          "term": {
            "scopeDefinitionKey": "travelRequest"
          }
        },
        {
          "nested": {
            "path": "variables",
            "query": {
              "bool": {
                "must": [
                  {
                    "term": {
                      "variables.name": "destination"
                    }
                  },
                  {
                    "match": {
                      "variables.textValue": "{destination}"
                    }
                  }
                ]
              }
            }
          }
        }
      ]
    }
  }
}