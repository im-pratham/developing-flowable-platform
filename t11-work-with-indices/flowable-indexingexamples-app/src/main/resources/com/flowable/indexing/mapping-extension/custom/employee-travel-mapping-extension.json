{
  "key": "employee-travel-mapping-extension",
  "extends": "case-instances",
  "version": 1,
  "properties": {
    "originGeoPoint": {
      "type": "geo_point"
    },
    "destinationGeoPoint": {
      "type": "geo_point"
    }
  },
  "variableExtractors": [
    {
      "filter": {
        "name": "travelRequest",
        "scopeDefinitionKey": "employeeTravelRequest"
      },
      "path": "/origin",
      "to": "extractedOrigin",
      "type": "string"
    },
    {
      "filter": {
        "name": "travelRequest",
        "scopeDefinitionKey": "employeeTravelRequest"
      },
      "path": "/destination",
      "to": "extractedDestination",
      "type": "string"
    }
  ]
}